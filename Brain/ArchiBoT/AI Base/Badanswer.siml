  <Concept Type="private" Name="BADANSWER-PRELIM">
    <Model>
      <Pattern>_</Pattern>
      <Response>
        <Think>
          <User Set="star">
            <Match />
          </User>
        </Think>
        <If User="star" Value="yes">	OK, what should I have said?<User Think:Set="concept">learning new answers</User></If>
        <ElseIf User="star" Value="no">	OK, let's forget it then.<User Think:Set="concept">learning new answers</User></ElseIf>
        <Else>Yes or No?</Else>
      </Response>
    </Model>
  </Concept>
