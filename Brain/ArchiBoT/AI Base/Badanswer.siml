  <Concept Type="private" Name="BADANSWER">
    <Model>
      <Pattern>_</Pattern>
      <Response>
        <Think>
          <User Set="star">
            <Match />
          </User>
          <User Set="response1">
            <Result />
          </User>
          <User Set="response2">
            <Result At="2" />
          </User>
          <If User="response2" Value="OK, what should I have said">
            <User Set="badanswer-newresp">
              <Request />
            </User>
          </If>
        </Think>
        <If User="response1" Value="OK, what should I have said" />
        <ElseIf User="star" Value="no">I'll try to remember to respond, "<User Get="badanswer-newresp" />" when you say, "<User Get="badanswer-input" />" <Remember><Model><Pattern><Process><Text Get="uppercase"><User Get="badanswer-input" /></Text></Process></Pattern><Response><Process><User Get="badanswer-newresp" /></Process></Response></Model></Remember><User Think:Set="concept">learning new answers</User></ElseIf>
        <ElseIf User="star" Value="yes">I'll try to remember to respond, "<User Get="badanswer-newresp" />" when you say, "<User Get="badanswer-input" />" if I have just said "<User Get="badanswer-that-full" />" <Remember><Model><Pattern><Process><Text Get="uppercase"><User Get="badanswer-input" /></Text></Process></Pattern><Previous><Process><Text Get="uppercase"><User Get="badanswer-that" /></Text></Process></Previous><Response><Process><User Get="badanswer-newresp" /></Process></Response></Model></Remember><User Think:Set="concept">learning new answers</User></ElseIf>
        <Else>Yes or No?</Else>
      </Response>
    </Model>
  </Concept>
