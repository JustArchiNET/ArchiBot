    <!--Default: Value that is returned if a Get function is used for an undefined variable-->
    <Variable Name="Default" Value="Unknown" />
    <!--Overrides older patterns with newer ones if set to true. Default: false (older is used over newer)-->
    <Variable Name="override-pattern" Value="true" />
    <!--Max number of characters in a request before it's automatically timed out -->
