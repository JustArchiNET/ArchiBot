<Siml>
  <!--BotSettings: Variables stored within this element are used by SIML Bot. To use a variable as an array use <Value> tag within a Variable-->
  <BotSettings>
    <!--Response that is added if No internal response is generated-->
    <Variable Name="Empty-Response" Value="Sorry, no response was generated." />
    <!--Determines the maximum time limit in milliseconds for processing a response-->
    <!--Note: For Syn Web Assistant this value is automatically overridden and is set to 4000 milliseconds-->
    <Variable Name="Timeout" Value="60000" />
    <!--Used to Separate variables in listed formats example 1, 2, 3-->
    <Variable Name="List-Separator" Value=", " />
    <!--Adds a delimiter when a List is requested example 1, 2, 3 AND 4-->
    <Variable Name="List-End" Value=" and " />
    <!--Culture: Is used to let SIML Bot know what environment culture is to be used.-->
